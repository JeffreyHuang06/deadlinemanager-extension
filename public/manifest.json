{
  "name": "Deadline Manager",
  "version": "1.0.0",
  "description": "Manage your deadlines so you don't forget them!",
  "permissions": ["storage"],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "background": {
    "scripts": ["background.ts"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'sha256-RVlQ4HUL5fqmYbj3A1hJToOdDiXLNxhzUvfslCru5zU=' 'localhost:8097'; object-src 'self'",
  "manifest_version": 2
}